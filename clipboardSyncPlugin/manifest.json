{
	"manifest_version": 2,
	"name": "剪贴同步",
	"version": "0.0.1",
	"description": "Clickboard Sync",
	"author": "FirYuen",
	"icons":
	{
		"48": "icon.png",
		"128": "icon.png"
	},
	"browser_action": 
	{
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"background": {
		"page": "background.html"
	 },
	"permissions": [ "background", "notifications", "clipboardRead", "clipboardWrite" ]

}


